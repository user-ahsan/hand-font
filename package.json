{
  "dependencies": {
    "@clerk/nextjs": "^6.31.4",
    "@headlessui/react": "^2.2.7",
    "@prisma/client": "^6.14.0",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.541.0",
    "next": "^15.5.0",
    "prisma": "^6.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "name": "hand-font",
  "version": "1.0.0",
  "description": "HandFont - Create beautiful handwritten fonts from your own handwriting",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon backend/src/index.ts",
    "dev:client": "npm run dev --prefix frontend",
    "dev:frontend": "npm run dev --prefix frontend",
    "dev:backend": "nodemon backend/src/index.ts",
    "build": "npm run build --prefix frontend",
    "build:frontend": "npm run build --prefix frontend",
    "start": "npm run start --prefix frontend",
    "start:frontend": "npm run start --prefix frontend",
    "install:components": "cd frontend && npm run install:components",
    "install:components:win": "cd frontend && install-components.bat",
    "install:components:unix": "cd frontend && chmod +x install-components.sh && ./install-components.sh",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["font", "handwriting", "typography", "design", "ai"],
  "author": "HandFont Team",
  "license": "ISC",
  "type": "commonjs",
  "workspaces": [
    "frontend",
    "backend"
  ]
}
